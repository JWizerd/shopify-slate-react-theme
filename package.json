{
  "name": "slate-react-starter",
  "version": "1.0.0",
  "private": true,
  "author": "Shopify Inc., Liron Navon, Jeremiah Wodke",
  "description": "A slightly modified version of the slate-react-starter repository, that includes a lot of packages included in the slate starter theme along with other useful js libraries and UI components. For those who like a little more opinion with their starter base themes.",
  "keywords": [
    "shopify",
    "theme",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@shopify/slate-tools": "^1.0.0-beta.14",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-shopify": "^16.6.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0"
  },
  "scripts": {
    "start": "slate-tools start",
    "watch": "slate-tools start --skipFirstDeploy",
    "build": "slate-tools build",
    "deploy": "slate-tools build && slate-tools deploy",
    "zip": "slate-tools build && slate-tools zip"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "lazysizes": "^4.1.1",
    "lodash-es": "^4.17.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
